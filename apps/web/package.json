{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "dev:webpack": "next dev --webpack",
    "build": "next build --webpack && node -e \"const fs=require('fs');const path=require('path');function cpR(s,d){if(!fs.existsSync(d))fs.mkdirSync(d,{recursive:true});fs.readdirSync(s).forEach(f=>{const sp=path.join(s,f),dp=path.join(d,f);fs.statSync(sp).isDirectory()?cpR(sp,dp):fs.copyFileSync(sp,dp)})};cpR('.next/static','.next/standalone/.next/static');cpR('public','.next/standalone/public');if(fs.existsSync('staticwebapp.config.json'))fs.copyFileSync('staticwebapp.config.json','.next/standalone/staticwebapp.config.json');if(fs.existsSync('backend-config.json'))fs.copyFileSync('backend-config.json','.next/standalone/backend-config.json')\"",
    "build:dev": "next build",
    "start": "next start",
    "lint": "eslint",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui"
  },
  "dependencies": {
    "@copilotkit/react-core": "^1.50.1",
    "@copilotkit/react-ui": "^1.50.1",
    "@copilotkit/runtime": "^1.50.1",
    "@copilotkit/runtime-client-gql": "^1.50.1",
    "@heroicons/react": "^2.2.0",
    "lucide-react": "^0.468.0",
    "next": "^16.1.1",
    "next-themes": "^0.4.6",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-markdown": "^10.1.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^16.1.1",
    "@playwright/test": "^1.57.0",
    "@tailwindcss/postcss": "^4",
    "@tailwindcss/typography": "^0.5.19",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20.19.27",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^4.5.2",
    "babel-plugin-react-compiler": "1.0.0",
    "eslint": "^9",
    "eslint-config-next": "^16.1.1",
    "eslint-config-prettier": "^10.1.8",
    "jsdom": "^25.0.0",
    "prettier": "^3.7.4",
    "tailwindcss": "^4",
    "typescript": "^5",
    "vitest": "^4.0.16"
  },
  "overrides": {
    "prismjs": "^1.30.0",
    "diff": "^8.0.3",
    "hono": "^4.11.4",
    "minimatch": ">=10.2.1"
  }
}
